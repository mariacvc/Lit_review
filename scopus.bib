@ARTICLE{Landrigan2023,
	author = {Landrigan, Philip J. and Raps, Hervé and Cropper, Maureen and Bald, Caroline and Brunner, Manuel and Canonizado, Elvia Maya and Charles, Dominic and Chiles, Thomas C. and Donohue, Mary J. and Enck, Judith and Fenichel, Patrick and Fleming, Lora E. and Ferrier-Pages, Christine and Fordham, Richard and Gozt, Aleksandra and Griffin, Carly and Hahn, Mark E. and Haryanto, Budi and Hixson, Richard and Ianelli, Hannah and James, Bryan D. and Kumar, Pushpam and Laborde, Amalia and Law, Kara Lavender and Martin, Keith and Mu, Jenna and Mulders, Yannick and Mustapha, Adetoun and Niu, Jia and Pahl, Sabine and Park, Yongjoon and Pedrotti, Maria-Luiza and Pitt, Jordan Avery and Ruchirawat, Mathuros and Seewoo, Bhedita Jaya and Spring, Margaret and Stegeman, John J. and Suk, William and Symeonides, Christos and Takada, Hideshige and Thompson, Richard C. and Vicini, Andrea and Wang, Zhanyun and Whitman, Ella and Wirth, David and Wolff, Megan and Yousuf, Aroub K. and Dunlop, Sarah},
	title = {The Minderoo-Monaco Commission on Plastics and Human Health},
