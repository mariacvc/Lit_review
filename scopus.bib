@ARTICLE{Rathinam2021,
	author = {Rathinam, Francis and Khatua, Sayak and Siddiqui, Zeba and Malik, Manya and Duggal, Pallavi and Watson, Samantha and Vollenweider, Xavier},
	title = {Using big data for evaluating development outcomes: A systematic map},
	year = {2021},
